


@{
    ViewBag.Title = "AvailabilityHistoricDashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@*<script src="~/Scripts/d3.min.js"></script>*@
<script src="~/Scripts/d3_v4.min.js"></script>
