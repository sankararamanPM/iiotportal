                                        barGroups
                                            .append('text')
                                            .attr('class', 'value')
                                            .attr('x', (a) => xScale(a.Shift_ID) + xScale.bandwidth() / 2)
                                            .attr('y', (a) => yScale(a.OEE) + -10)
                                            .attr('text-anchor', 'middle')
                                            .text((a) => `${a.OEE}`)

                                        svg.append('text')
                                            .attr('class', 'label')
                                            .attr('x', -(height / 2) - margin)
                                            .attr('y', margin / 2.4)
                                            .attr('transform', 'rotate(-90)')
                                            .attr('text-anchor', 'middle')
                                            .text('OEE(in % )')

                                        svg.append('text')
                                            .attr('class', 'label')
                                            .attr('x', width / 2 + margin)
                                            .attr('y', height + margin * 1.7)
                                            .attr('text-anchor', 'middle')
                                            .text('Shift Id')
                                    }
                                    else {
                                        sample = "";
                                        $('#monthly_line').text(linecode);
                                        $('#monthly_subsystem').text(machinecode);
                                        $(".month_shift").empty();

                                        var div_width = 400;
                                        var div_height = 400;
                                        var svg = d3.select(".month_shift").append("svg")
                                            .attr("width", div_width)
                                            .attr("height", div_height)
                                            .attr("preserveAspectRatio", "xMidYMid")
                                            .append("g")
                                            .attr("transform", "translate(" + (div_width / 2 - 50) + "," + (div_height / 2 - 50) + ")");

                                        svg.append("text")
                                            .text("No Data Available")
                                            .style("font-size", "40px");
                                    }
                                },
                                error: function (result) {
                                    //console.log(result);
                                }
                            });
                        },
                        error: function (result) {
                            //console.log(result);
                        }
                    });
                },
                error: function (result) {
                    //console.log(result);
                }
            });
        }
    }
</script>
